package com.psl;

public class Client {

	public static int addSplitedNumber(int n) {

		int ress, addSqrs = 0;

		while (n > 0) {
			ress = n % 10;
			n = n / 10;
			addSqrs = addSqrs + ress;

		}
		return addSqrs;
	}

	public static int getIsometricValue(int n, int m) {

